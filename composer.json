{
    "name": "sierratecnologia/writelabel",
    "description": "Game",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "writelabel",
        "points",
        "pointable",
        "laravel",
        "loyalty",
        "reward"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/writelabel/",
    "support": {
        "email": "help@sierratecnologia.com",
        "issues": "https://github.com/sierratecnologia/writelabel/issues",
        "source": "https://github.com/sierratecnologia/writelabel",
        "docs": "https://github.com/sierratecnologia/writelabel/blob/master/README.md"
    },
    "authors": [
        {
            "name": "Ricardo Sierra",
            "email": "ricardo@ricasolucoes.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        },
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        }
    ],
    "require": {
        "sierratecnologia/audit": "dev-master",
        "sierratecnologia/pedreiro": "dev-master",
        "sierratecnologia/muleta": "dev-master",
        "markrogoyski/math-php": "*",
        "php-ai/php-ml": ">=0.8",
        "spatie/laravel-event-sourcing": "^4.3",
        "jordanbrauer/unit-converter": "^0.8.4",
        "lorisleiva/laravel-actions": "^1.1"
    },
    "require-dev": {
        "illuminate/database": "^6.0|^7.0|^8.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "doctrine/dbal": "^2.10",
        "fakerphp/faker": "^1.5",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.4",
        "codedungeon/phpunit-result-printer": ">=0.27.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "vimeo/psalm": "^4.0",
        "phpro/grumphp": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "WriteLabel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tecnico\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": [],
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "WriteLabel\\WriteLabelProvider"
            ]
        }
    }
}
